{
    "provider": {
        "name": "review_service"
    },
    "consumer": {
        "name": "ec_app"
    },
    "interactions": [
        {
            "description": "A request for ratings for a product",
            "request": {
                "method": "GET",
                "path": "/ratings",
                "query": "productId=8&userName=l",
                "matchingRules": {
                    "$.queryproductId": {
                        "match": "regex",
                        "regex": "\\d+"
                    },
                    "$.queryuserName": {
                        "match": "regex",
                        "regex": "[a-z]+"
                    }
                }
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": [
                    {
                        "productId": "123456",
                        "rating": "5",
                        "userName": "ben"
                    }
                ],
                "matchingRules": {
                    "$.body": {
                        "match": "type",
                        "min": 0
                    },
                    "$.body[*].productId": {
                        "match": "regex",
                        "regex": "^[0-9][0-9][0-9][0-9][0-9][0-9]$"
                    },
                    "$.body[*].userName": {
                        "match": "regex",
                        "regex": "[a-z]+"
                    },
                    "$.body[*].rating": {
                        "match": "regex",
                        "regex": "^[1-5]$"
                    }
                }
            },
            "providerState": "The ratings in Review service are ready"
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "2.0.0"
        },
        "pact-jvm": {
            "version": "3.5.0"
        }
    }
}